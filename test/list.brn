let length l =
  let rec aux acc = function
  | [] => acc
  | h :: t => aux (acc + 1) t
  in
  aux 0 l

let rev l =
  let rec aux acc = function
    | [] => acc
    | h :: t => aux (h :: acc) t
  in aux l

let map f l =
  let rec iter acc = function
    | [] => acc
    | h :: t => iter (f h :: acc) t
  in rev (iter [] l)